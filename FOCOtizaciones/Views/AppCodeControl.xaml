<UserControl x:Class="FOCOtizaciones.AppCodeControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FOCOtizaciones"
             mc:Ignorable="d" d:DesignWidth="300" Height="27">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="76*"/>
            <ColumnDefinition Width="73*"/>
            <ColumnDefinition Width="72*"/>
            <ColumnDefinition Width="79*"/>
        </Grid.ColumnDefinitions>
        <TextBox x:Name="textBoxID" Margin="0,0,0,0" TextWrapping="Wrap" Text="TextBox" Grid.Column="0"/>
        <TextBox x:Name="textBoxName" Margin="0,0,0,0" TextWrapping="Wrap" Text="TextBox" Grid.Column="1"/>
        <TextBox x:Name="textBoxPrice" Margin="0,0,0,0" TextWrapping="Wrap" Text="TextBox" Grid.Column="2"/>
        <Button x:Name="checkBoxEnable" Margin="0,0,0,0" Content="Enable" Grid.Column="3"/>
    </Grid>
</UserControl>
